{
  "name": "dominant",
  "version": "0.1.3",
  "source": "./src/Dominant.ts",
  "main": "./dist/dominant",
  "module": "./dist/dominant.mjs",
  "types": "./dist/types/src/dominant.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dominant.mjs",
      "require": "./dist/dominant.js",
      "types": "./dist/types/src/Dominant.d.ts",
      "source": "./src/Dominant.ts"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "dist",
    "index.js.flow",
    "bsconfig.json"
  ],
  "scripts": {
    "doc-install": "npm install --no-save bsdoc",
    "docs:build": "doc-install && bsdoc build Dominant && bsdoc support-files && prettier --write 'docs/**/*.html'",
    "check:ts": "tsc --noEmit",
    "check:flow": "flow focus-check ./src/flow/Dominant_flow_test.js",
    "check": "run-s check:ts check:flow",
    "bs:clean": "bsb -clean-world",
    "bs:build": "bsb -make-world",
    "bs:watch": "BS_WATCH_CLEAR=true bsb -make-world -w",
    "refmt": "bsrefmt --in-place **/*.{re,rei}",
    "bundle:clean": "rimraf dist node_modules/.cache",
    "bundle": "rollup -c rollup.config.js",
    "clean": "run-s bs:clean bundle:clean",
    "build": "run-s bs:build bundle flowgen",
    "prepublishOnly": "run-s clean build check test",
    "test": "is-ci-cli test:ci test:watch",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "rollup:build": "rollup -c rollup.config.js",
    "flowgen": "./scripts/generate-flow-files.js",
    "test:coverage": "jest --coverage",
    "build-demo": "npm run build && webpack",
    "dev-demo": "webpack serve"
  },
  "keywords": [
    "Rescript",
    "Reason",
    "image",
    "Colors"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
    "@babel/core": "7.14.2",
    "@babel/plugin-syntax-typescript": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "7.14.2",
    "@glennsl/bs-jest": "^0.7.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.23",
    "babel-jest": "26.6.3",
    "babel-plugin-closure-elimination": "^1.3.2",
    "bs-platform": "^9.0.2",
    "flow-bin": "^0.151.0",
    "flowgen": "^1.14.1",
    "gentype": "^3.50.0",
    "globby": "^11.0.1",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.2.5",
    "is-ci-cli": "2.2.0",
    "jest": "26.6.3",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "2.48.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copier": "1.1.0",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-prettier": "^2.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "bugs": {
    "url": "https://github.com/Swapcard-Corporation/dominant/issues"
  },
  "homepage": "https://github.com/Swapcard-Corporation/dominant#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Swapcard-Corporation/dominant.git"
  },
  "lint-staged": {
    "*.{d.ts,js}": [
      "prettier --write"
    ],
    "*.{re,rei}": [
      "bsrefmt --in-place"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testRegex": "(src/.*(\\.|/)(test|spec))\\.(t|j)sx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "\\.jsx?$": "<rootDir>/scripts/jest-transform-esm.js",
      "\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!bs-platform)"
    ],
    "collectCoverageFrom": [
      "!<rootDir>/src/*.{ts,bs.js}",
      "!<rootDir>/src/**/*.{shim,gen}.{ts,tsx}",
      "<rootDir>/src/*.{js,ts,tsx}"
    ]
  }
}
